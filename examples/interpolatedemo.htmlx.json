{ "div": {
		"id": "{{data.innerdata.id}}",
		"style": {
			"width": "{{style.width}}px",
			"height": "{{style.height}}px",
			"padding": "10px",
			"border": "1px solid black",
			"example-undefined": "{{data.non.existant}}"
		},
		"textContent": "Here is some text that will be displayed.",
		"childNodes": [
			{"button": {
				"id": "button-one",
				"onclick": "alert(`You've clicked ${this.id}`)",
				"textContent": "1"
			}},
			{"button": {
				"id": "button-two",
				"onclick": "alert(`You've clicked ${this.id}`)",
				"textContent": "2"
			}}
		]
	}
}